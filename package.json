{
  "name": "botium-connector-agentforce",
  "version": "1.0.0",
  "description": "Botium connector for Salesforce Agentforce",
  "main": "index.js",
  "scripts": {
    "build": "echo \"Placeholder\"",
    "lint": "eslint index.js",
    "lint:fix": "eslint --fix index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "INTEGRATION_TEST=true jest test/integration.test.js --detectOpenHandles",
    "test:all": "npm test && npm run test:integration",
    "demo": "node demo.js",
    "publish:premium-dist": "npm run lint && npm pack && cross-env rm -f $PUBLISH_DIR/packages/botium-connector-agentforce*.tgz && cross-env mkdir -p $PUBLISH_DIR/packages && cross-env mv botium-connector-agentforce*.tgz $PUBLISH_DIR/packages"
  },
  "keywords": [
    "botium",
    "connector",
    "salesforce",
    "agentforce",
    "chatbot",
    "testing",
    "ai"
  ],
  "author": "Botium Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "debug": "^4.3.4",
    "lodash": "^4.17.21"
  },
  "peerDependencies": {
    "botium-core": ">=2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "botium-core": "https://github.com/codeforequity-at/botium-core.git#develop",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-standard": "^17.1.0",
    "jest": "^29.7.0"
  },
  "optionalDependencies": {
    "dotenv": "^16.6.1"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
